@page
@model HilleroedSejlKlub_Project.Pages.EventsModel
@using HilleroedSejlKlub_Project.Models

@{
}
<h1>Events</h1>

<ul>
    @foreach (Event ev in Model.Events)
    {
        <li>
            <a asp-page="/EventDetails" asp-route-id="@ev.Id">@ev.Name</a> - @ev.Date.ToShortDateString()
        </li>
    }
</ul>